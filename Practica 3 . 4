program 4; 
type
 linea=record
 num:integer;
 minutos:integer;
 mb:integer;
 end;
 cliente=record
 cod:integer;
 cant:integer;
 lineas:linea;
 end;
 
 procedure leerlinea(var l:linea; cantidad:integer);
 var
 i:integer;
 begin
 for i:=1 to cantidad do
   read(l.num);
   read(l.minutos);
   read(l.mb);
 end;
   
 procedure leer (var c:cliente);
 var
 i:integer;
 min,mbs:real;
 begin
 for i:=1 to 9300 do
   read(c.cod);
   read(c.cant);
   leerlinea(c.lineas,c.cant);
   min:=c.lineas.minutos*1.35;
   mbs:=c.lineas.mb*3.40;
   writeln(min,mbs);
   end;
 
 var
 clientes:cliente;
 begin
    leer (clientes);
 end
    
   
